<!-- Google Analytics -->
<script src="<%=request.protocol=='https' ? 'https://ssl' : 'http://www' %>.google-analytics.com/ga.js" type="text/javascript"></script>
<script type="text/javascript">
try {
var pageTracker = _gat._getTracker("<%=escape_javascript analytics_code%>");
pageTracker._trackPageview(<%= error_code.blank? ? '' : "\"/#{error_code}.html?\"+document.location" %>);
} catch(err) {}
</script>
